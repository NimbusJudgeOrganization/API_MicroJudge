#default factor
TLMOD[calibrafactor]=1.35

#modifier to a language [specially if it does not have a solution but is
#known to be slow]

# This will accept solutions that take up to 0.02 more than the default TL,
# but will not modify the current TL of the problem for this language
TLMOD[java.drift]=0.02

# This will ADD this much to the TL of the problem. For instance, this
# sample problem has a TL of 0.01 for spim it will be 1.01. It will sum this
# even if there is a solution calibrated to this language
TLMOD[spim.sum]=1
